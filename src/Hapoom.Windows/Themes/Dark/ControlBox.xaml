<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:w="clr-namespace:Hapoom.Windows">
    <SolidColorBrush x:Key="{x:Static w:ResourceKeys.ControlBoxForeground}"                Color="#FF3F3F46"/>
    <SolidColorBrush x:Key="{x:Static w:ResourceKeys.ControlBoxForegroundMouseOver}"       Color="Black"/>
    <SolidColorBrush x:Key="{x:Static w:ResourceKeys.ControlBoxForegroundPressed}"         Color="White"/>

    <SolidColorBrush x:Key="{x:Static w:ResourceKeys.ControlBoxBackground}"                Color="#FF252525"/>
    <SolidColorBrush x:Key="{x:Static w:ResourceKeys.ControlBoxBackgroundMouseOver}"       Color="#FF3F3F41"/>
    <SolidColorBrush x:Key="{x:Static w:ResourceKeys.ControlBoxBackgroundPressed}"         Color="#FF0079CB"/>

    <SolidColorBrush x:Key="{x:Static w:ResourceKeys.ControlBoxForegroundMouseOverClose}"  Color="White"/>
    <SolidColorBrush x:Key="{x:Static w:ResourceKeys.ControlBoxForegroundPressedClose}"    Color="White"/>
    <SolidColorBrush x:Key="{x:Static w:ResourceKeys.ControlBoxBackgroundMouseOverClose}"  Color="#FFE81123"/>
    <SolidColorBrush x:Key="{x:Static w:ResourceKeys.ControlBoxBackgroundPressedClose}"    Color="#FFF1707A"/>
</ResourceDictionary>