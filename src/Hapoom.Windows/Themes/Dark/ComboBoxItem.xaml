<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:w="clr-namespace:Hapoom.Windows">
    <!-- Dark -->
    <SolidColorBrush x:Key="{x:Static w:ResourceKeys.ComboBoxItemForegroundKey}"            Color="#FF494949"/>
    <SolidColorBrush x:Key="{x:Static w:ResourceKeys.ComboBoxItemForegroundMouseOverKey}"   Color="White"/>
    <SolidColorBrush x:Key="{x:Static w:ResourceKeys.ComboBoxItemForegroundSelectedKey}"    Color="#FF0079CB"/>
    <SolidColorBrush x:Key="{x:Static w:ResourceKeys.ComboBoxItemBackgroundKey}"            Color="Black"/>
    <SolidColorBrush x:Key="{x:Static w:ResourceKeys.ComboBoxItemBackgroundMouseOverKey}"   Color="#FF0079CB"/>
    <SolidColorBrush x:Key="{x:Static w:ResourceKeys.ComboBoxItemBackgroundSelectedKey}"    Color="#FF0079CB"/>
</ResourceDictionary>