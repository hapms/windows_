<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:w="clr-namespace:Hapoom.Windows">
    <!-- Dark -->
    <Color           x:Key="{x:Static w:ResourceKeys.WindowDropShadowColorKey}">#FFBEDDF2</Color>
    <Color           x:Key="{x:Static w:ResourceKeys.WindowDropShadowInActiveColorKey}">Silver</Color>
    <SolidColorBrush x:Key="{x:Static w:ResourceKeys.WindowLogoBackgroundKey}" Color="#FF494949"/>
    <SolidColorBrush x:Key="{x:Static w:ResourceKeys.WindowBackgroundKey}"     Color="#FF252525"/>
    <SolidColorBrush x:Key="{x:Static w:ResourceKeys.WindowBorderKey}"         Color="#FF0079CB"/>
    <SolidColorBrush x:Key="{x:Static w:ResourceKeys.WindowBorderInActiveKey}" Color="#FF3F3F46"/>
    <SolidColorBrush x:Key="{x:Static w:ResourceKeys.WindowTitleBorder}"       Color="#FF3F3F46"/>
    <SolidColorBrush x:Key="{x:Static w:ResourceKeys.WindowTitleForeground}"   Color="#FF494949"/>
    <SolidColorBrush x:Key="{x:Static w:ResourceKeys.WindowTitleBackground}"   Color="#FF252525"/>
</ResourceDictionary>